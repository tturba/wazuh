<group name="paloalto,">

  <!-- TRAFFIC -->
  <rule id="100000" level="3">
    <decoded_as>paloalto-traffic</decoded_as>
    <description>Palo Alto TRAFFIC log</description>
  </rule>

  <!-- THREAT -->
  <rule id="100001" level="7">
    <decoded_as>paloalto-threat</decoded_as>
    <description>Palo Alto THREAT detected</description>
    <group>attack,</group>
  </rule>

  <!-- SYSTEM -->
  <rule id="100002" level="4">
    <decoded_as>paloalto-system</decoded_as>
    <description>Palo Alto SYSTEM log</description>
  </rule>

  <!-- CONFIG -->
  <rule id="100003" level="5">
    <decoded_as>paloalto-config</decoded_as>
    <description>Palo Alto configuration change</description>
    <group>config_change,</group>
  </rule>

  <!-- HIPMATCH -->
  <rule id="100004" level="5">
    <decoded_as>paloalto-hipmatch</decoded_as>
    <description>Palo Alto HIPMATCH detected</description>
  </rule>

  <!-- Nieznany log -->
  <rule id="100099" level="2">
    <decoded_as>paloalto</decoded_as>
    <description>Unclassified Palo Alto log</description>
  </rule>

</group>
